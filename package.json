    "babel-loader": "^8.0.2",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.0",
    "eslint": "^5.3.0",
    "eslint-config-codex": "github:codex-team/eslint-config",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "highlight.js": "^9.13.1",
    "husky": "^1.1.2",
    "mini-css-extract-plugin": "^0.4.3",
    "mocha": "^5.2.0",
